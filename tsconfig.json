{
    "compilerOptions": {
      "sourceMap": true,
      "jsx": "react",
      "module": "esnext",
      "target": "es5",
      "lib": ["es6", "dom"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }